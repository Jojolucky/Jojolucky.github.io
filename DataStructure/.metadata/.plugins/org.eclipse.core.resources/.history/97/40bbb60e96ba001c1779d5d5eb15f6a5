package queue;

// 队的顺序为  先进先出
public class ArrayQueue {
	private int maxSize; // 队的大小
	private int front; //队的头部元素
	private int rare; // 队的尾部元素
	private int[] arr; // 用于存储数据，模拟队列
	
	public ArrayQueue(int arrMaxSize) {
		maxSize = arrMaxSize;
		front = -1;
		rare = -1;
		arr = new int[maxSize];
				
		
	}
	
	
	
}
